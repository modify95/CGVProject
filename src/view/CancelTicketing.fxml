<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.VBox?>

<VBox spacing="10.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.CancelTicketingController">
   <children>
      <Label text="예매번호" />
      <HBox alignment="CENTER" spacing="5.0">
         <children>
            <TextField fx:id="txtTNum1" alignment="CENTER" prefWidth="80.0" />
            <Label text="-" />
            <TextField fx:id="txtTNum2" alignment="CENTER" prefWidth="80.0" />
            <Label text="-" />
            <TextField fx:id="txtTNum3" alignment="CENTER" prefWidth="80.0" />
            <Label text="-" />
            <TextField fx:id="txtTNum4" alignment="CENTER" prefWidth="80.0" />
         </children>
      </HBox>
      <HBox alignment="CENTER" spacing="10.0">
         <children>
            <Button fx:id="btnCancelTicketing" mnemonicParsing="false" prefHeight="40.0" prefWidth="90.0" text="예약 취소" />
            <Button fx:id="btnExit" mnemonicParsing="false" prefHeight="40.0" prefWidth="90.0" text="닫기" />
         </children>
         <VBox.margin>
            <Insets top="20.0" />
         </VBox.margin>
      </HBox>
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
